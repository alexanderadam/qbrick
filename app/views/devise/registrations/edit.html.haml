%h2='Change Password'
= simple_form_for(resource, :as => resource_name, :url => admin_registration_path(resource_name), :html => { :method => :patch }) do |f|
  = devise_error_messages!
  .form-inputs
    = f.input :current_password
    = f.input :password
    = f.input :password_confirmation
  .form-actions
    = f.button :submit, 'Save'
